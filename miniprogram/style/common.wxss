.header-search{
  width:100%;
  height: 80rpx;
  /* background-color: #515151; */
  border-bottom: 1rpx solid rgba(0, 0 , 0 , 0.1);
  border-top: 1rpx solid rgba(0, 0 , 0 , 0.1);
  display: flex;
  justify-content: center;
  align-items: center;
}
.header-search-circle{
  width: 90%;
  height: 85%;
  background-color: #dbdbdb;
  /* opacity: 0.1; */
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.header-search-circle-text{
  color: #707070;
}
.header-search-circle-icon{
  width: 56rpx;
  height: 56rpx;
  /* z-index: 100;
  opacity: 1; */
}