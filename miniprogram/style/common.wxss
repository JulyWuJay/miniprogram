/* 顶部搜索框 无法输入字符 */
.header-search{
  width:100%;
  height: 8%;
  /* background-color: #515151; */
  border-bottom: 1rpx solid rgba(0, 0 , 0 , 0.1);
  border-top: 1rpx solid rgba(0, 0 , 0 , 0.1);
  display: flex;
  justify-content: center;
  align-items: center;
}
.header-search-circle{
  width: 90%;
  height: 70%;
  background-color: #dbdbdb;
  /* opacity: 0.1; */
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.header-search-circle-text{
  color: #515151;
}
.header-search-circle-icon{
  width: 48rpx;
  height: 48rpx;
}
