/* miniprogram/pages/people/student/individual/individual.wxss */
@import "../../../../style/common.wxss";

page{
  width: 100%;
  height: 100%;
  background: #f6f6f6;
}
.main{
  width: 100%;
  height: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}
.group{
  width: 90%;
  height: 100rpx;  
  background: #fff;
  display: flex;
  flex-direction: grow;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f6f6f6;
}
.group > text {
  font-size: 48rpx;
}